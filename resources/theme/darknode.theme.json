    },
    "CompletionPopup": {
      "foreground": "#FFFFFF",
      "matchForeground": "#ffd796",
      "selectionBackground": "#507939",
      "selectionInactiveBackground": "#507939",
      "Advertiser.background": "color-bg"
    },
    "FileColor.Green": "#13980d"
