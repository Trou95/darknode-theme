{
  "name": "Darknode Theme",
  "author": "Trou95",
  "editorScheme": "/theme/darknode.xml",
  "dark": true,
  "colors": {
    "color-bg": "#282C34"
  },
  "ui": {
    "*": {
      "background": "color-bg"
    },
    "Tree.selectionBackground": "#3c9f4a",
    "Tree.selectionForeground": "#000000",
    "Tree.selectionInactiveBackground": "#41556e",
    "Borders.color": "#0e0e0e",
    "EditorTabs": {
      "background": "#1f1f1f",
      "underlineColor": "#10a429",
      "underlinedTabBackground": "#414141"
    },
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "#1f1f1f"
      },
      "HeaderTab.underlineColor": "#6a8d6a",
      "HeaderTab.padding": "20,20,20,20",
      "Button": {
        "hoverBackground": "#6a8d6a",
        "selectedBackground": "#6a8d6a"
      }
    },
    "Button": {

    },
    "ActionButton": {
      "hoverBackground": "#6a8d6a",
      "pressedBackground": "#6a8d6a"
    },
    "Popup": {
      "background": "color-bg",
      "borderColor": "color-bg",
